{
  "name": "@outray/vite",
  "version": "0.1.0",
  "description": "Vite plugin to automatically expose your dev server via Outray tunnel",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "tunnel",
    "outray",
    "ngrok",
    "localhost",
    "development"
  ],
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.0"
  },
  "author": "akinloluwami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akinloluwami/outray.git",
    "directory": "packages/vite-plugin"
  },
  "homepage": "https://outray.dev",
  "bugs": {
    "url": "https://github.com/akinloluwami/outray/issues"
  }
}
