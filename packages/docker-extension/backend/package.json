{
    "name": "outray-docker-extension-backend",
    "version": "0.0.1",
    "scripts": {
        "build": "tsc && cpy \"src/drizzle/**/*\" dist/drizzle --parents",
        "dev": "tsx watch src/main.ts",
        "start": "node dist/main.js",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate"
    },
    "dependencies": {
        "@libsql/client": "^0.17.0",
        "@outray/core": "^0.0.1",
        "dotenv": "^17.2.4",
        "drizzle-orm": "^0.45.1",
        "express": "^5.2.1",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@types/express": "^5.0.6",
        "@types/node": "^22.0.0",
        "cpy-cli": "^7.0.0",
        "drizzle-kit": "^0.31.8",
        "tsx": "^4.21.0",
        "typescript": "^5.0.0"
    }
}