{
  "name": "@outray/core",
  "version": "0.0.1",
  "description": "Core tunnel client for Outray - shared between CLI and framework plugins",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "outray",
    "tunnel",
    "ngrok",
    "localhost",
    "proxy",
    "websocket"
  ],
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "author": "akinloluwami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akinloluwami/outray.git",
    "directory": "packages/core"
  },
  "homepage": "https://outray.dev",
  "bugs": {
    "url": "https://github.com/akinloluwami/outray/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
