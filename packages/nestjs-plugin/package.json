{
    "name": "@outray/nest",
    "version": "0.0.2",
    "description": "NestJS plugin to automatically expose your dev server via Outray tunnel",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "nestjs",
        "nest",
        "tunnel",
        "outray",
        "localhost",
        "development"
    ],
    "peerDependencies": {
        "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
        "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0"
    },
    "dependencies": {
        "@outray/core": "^0.0.2"
    },
    "devDependencies": {
        "@nestjs/common": "^11.1.12",
        "@nestjs/core": "^11.1.12",
        "@types/node": "^25.0.10",
        "rxjs": "^7.8.2",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/akinloluwami/outray.git",
        "directory": "packages/nestjs-plugin"
    },
    "homepage": "https://outray.dev",
    "bugs": {
        "url": "https://github.com/akinloluwami/outray/issues"
    }
}
