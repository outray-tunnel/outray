{
  "name": "@outray/express",
  "version": "0.0.1",
  "description": "Express middleware to automatically expose your dev server via Outray tunnel",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "express",
    "express-middleware",
    "tunnel",
    "outray",
    "ngrok",
    "localhost",
    "development"
  ],
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@outray/core": "^0.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "express": "^4.19.2",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "author": "akinloluwami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/outray-tunnel/outray.git",
    "directory": "packages/express-plugin"
  }
}
